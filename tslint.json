{
  "rulesDirectory": [],
  "extends": [
    "tslint:latest",
    "tslint:recommended",
    "tslint-react",
    "tslint-react-a11y"
  ],
  "linterOptions": {
    "exclude": [
      "./src/__mocks__/*",
      "./src/__mockData__/*",
      "./src/**/__mocks__/*",
      "./src/**/__mockData__/*"
    ]
  },
  "rules": {
    "align": [
      false
    ],
    "ban": false,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": [
      false,
      "always-prefix"
    ],
    "jsdoc-format": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "label-position": true,
    "react-a11y-anchors": false,
    "react-a11y-input-elements": false,
    "no-object-literal-type-assertion": false,
    "max-line-length": [
      false,
      {
        "limit": 120,
        "check-regex": false,
        "check-strings": false
      }
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "switch-default": true,
    "trailing-comma": [
      false
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "adjacent-overload-signatures": true,
    "typedef": [
      true,
      "parameter",
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case",
      "allow-snake-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "no-submodule-imports": false,
    "array-type": false,
    "ban-types": false,
    "callable-types": {
      "severity": "warning"
    },
    "interface-over-type-literal": {
      "severity": "warning"
    },
    "jsx-boolean-value": false,
    "jsx-key": {
      "severity": "warning"
    },
    "jsx-wrap-multiline": false,
    "jsx-self-close": {
      "severity": "warning"
    },
    "max-classes-per-file": {
      "severity": "warning"
    },
    "member-access": false,
    "no-angle-bracket-type-assertion": {
      "severity": "warning"
    },
    "no-empty-interface": {
      "severity": "warning"
    },
    "no-namespace": {
      "severity": "none"
    },
    "no-unused-expression": {
      "severity": "warning"
    },
    "no-var-keyword": true,
    "object-literal-sort-keys": false,
    "only-arrow-functions": {
      "severity": "warning"
    },
    "prefer-for-of": {
      "severity": "warning"
    },
    "no-implicit-dependencies": false
  },
  "prettier": true,
  "jsx-no-multiline-js": false,
  "jsx-no-lambda": false,
  "import-name": {
    "severity": "none"
  },
  "no-boolean-literal-compare": false
}
