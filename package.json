{
  "name": "tv-app",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test -- --watch --passWithNoTests",
    "test:coverage": "npm run test -- --coverage --passWithNoTests",
    "ts:check": "tsc --noEmit",
    "lint": "eslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.common.json --fix"
  },
  "dependencies": {
    "axios": "0.20.0",
    "lodash-es": "4.17.15",
    "next": "9.5.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "1.27.0",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@types/axios": "0.14.0",
    "@types/classnames": "2.2.10",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.15",
    "@types/jsdom": "16.2.3",
    "@types/lodash-es": "4.17.3",
    "@types/node": "14.14.2",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/testing-library__dom": "7.5.0",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/testing-library__react": "10.2.0",
    "@types/testing-library__react-hooks": "3.4.1",
    "@types/testing-library__user-event": "4.2.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-plugin-require-context-hook": "1.0.0",
    "eslint": "7.4.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "0.1.2",
    "imagemin-lint-staged": "0.4.0",
    "jest": "26.6.0",
    "jest-config": "26.6.0",
    "jest-dom": "4.0.0",
    "jsdom": "16.3.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.2.11",
    "postcss-calc": "7.0.2",
    "postcss-comment": "2.0.0",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-load-plugins": "2.3.0",
    "postcss-preset-env": "6.7.0",
    "postcss-pxtorem": "5.1.1",
    "postcss-safe-parser": "4.0.2",
    "sass-resources-loader": "2.0.3",
    "ts-jest": "26.4.1",
    "tslint": "6.1.3",
    "tslint-react": "5.0.0",
    "tslint-react-a11y": "1.1.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run ts:check && jest --coverage --coverageReporters=text-summary --passWithNoTests"
    }
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run test -- --bail --findRelatedTests"
    ]
  }
}
